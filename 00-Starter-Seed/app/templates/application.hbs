<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      {{#link-to 'index' classNames='navbar-brand'}}
        Home
      {{/link-to}}
    </div>
    <ul class="nav navbar-nav">
      {{#link-to 'protected' tagName='li'}}
        <a href="#">Protected Page</a>
      {{/link-to}}
      {{#if session.isAuthenticated}}
        {{#link-to 'profile' tagName='li'}}
          <a href="#">Profile</a>
        {{/link-to}}
      {{/if}}
    </ul>
    {{#if session.isAuthenticated}}
      <button class="btn btn-danger navbar-btn pull-right" {{ action 'logout' }}>Sign out</button>
    {{else}}
      <button class="btn btn-info navbar-btn pull-right" {{ action 'transitionToLoginRoute' }}>Sign in</button>
    {{/if}}
  </div>
</nav>
<div class="row text-center">
  <div class="col-md-12">
    {{outlet}}
  </div>
</div>
